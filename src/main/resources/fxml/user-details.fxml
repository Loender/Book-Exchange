<?xml version="1.0" encoding="UTF-8"?>

        <?import javafx.geometry.Insets?>
        <?import javafx.scene.control.Label?>
        <?import javafx.scene.control.ListView?>
        <?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER_LEFT" spacing="10" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.lab12dub2.controller.UserDetailsController">
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>
    <Label fx:id="usernameLabel" text="Username: "/>
    <Label fx:id="emailLabel" text="Email: "/>
    <Label fx:id="firstNameLabel" text="First Name: "/>
    <Label fx:id="lastNameLabel" text="Last Name: "/>
    <Label fx:id="phoneLabel" text="Phone: "/>
    <Label fx:id="dateOfBirthLabel" text="Date of Birth: "/>
    <Label text="Books Owned:"/>
    <ListView fx:id="booksListView" prefHeight="300.0" prefWidth="300.0"/>
    <Label text="Ratings Received:"/>
    <ListView fx:id="ratingsListView" prefHeight="300.0" prefWidth="300.0"/> <!-- Added ListView for ratings -->
</VBox>